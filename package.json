  "description": "Your favourite library for validating incoming data in express.js",
  "license": "MIT",
  "files": [
    "build/**/*"
  ],
  "main": "build/index",
  "types": "build/index",
  "scripts": {
    "lint:check": "eslint --max-warnings=7 .",
    "lint:fix": "eslint --max-warnings=7 --fix .",
    "valid": "yarn run build:dev && yarn run lint:fix",
    "test:all": "npx jest",
    "test:cov": "npx jest --coverage",
    "test:cov:badge": "npx coverage-badges --source jest/coverage/coverage-summary.json --output jest/badges/coverage.svg",
    "build:prod": "rm -rf ./build && tsc -p tsconfig.build.json && npx tsc-alias -p tsconfig.build.json",
    "build:dev": "rm -rf ./build && tsc -p tsconfig.json && npx tsc-alias -p tsconfig.json"
  },
  "peerDependencies": {
    "@types/formidable": "^2.0.5",
