    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.2",
    "formidable": "^2.0.1",
    "jest": "^27.4.5",
    "multer": "^1.4.5-lts.1",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.7.0",
