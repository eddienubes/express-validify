{
  "name": "validness",
  "version": "0.1.18",
  "engines": {
    "node": ">=16.5"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddienubes/validness.git"
  },
  "homepage": "https://github.com/eddienubes/validness#readme",
  "author": "Eddie Nubes <cool.gegeg@gmail.com>",
  "bugs": {
    "url": "https://github.com/eddienubes/validness/issues"
  },
  "keywords": [
    "express",
    "dto",
    "DTO",
    "validation",
    "data",
    "incoming data",
    "multer",
    "formidable",
    "query",
    "body",
    "form",
    "multipart/form-data",
    "nodejs",
    "node",
    "free",
    "quora",
    "tweet",
    "github",
    "opensource",
    "source",
    "star",
    "nestjs",
    "class-validator",
    "class-transformer",
    "transformer",
    "validator",
    "validness",
    "valid",
    "correct",
    "proper",
    "next"
  ],
  "description": "Your favourite library for validating incoming data in express.js",
  "license": "MIT",
  "files": [
    "build/**/*"
  ],
  "main": "build/cjs/index.js",
  "types": "build/cjs/index",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    },
    "./*": {
      "import": "./build/esm/*.js",
      "require": "./build/cjs/*.js"
    }
  },
  "scripts": {
    "lint:check": "eslint --max-warnings=50 .",
    "lint:fix": "eslint --max-warnings=50 --fix .",
    "valid": "yarn run build:dev && yarn run lint:fix",
    "test:all": "npx vitest run",
    "test:ci": "npx vitetest run --coverage",
    "build:prod:cjs": "tsc -p tsconfig.cjs.json && npx tsc-alias -p tsconfig.cjs.json",
    "build:prod:esm": "tsc -p tsconfig.esm.json && npx tsc-alias -p tsconfig.esm.json",
    "build:prod": "rm -rf ./build && pnpm run build:prod:cjs && pnpm run build:prod:esm",
    "build:dev": "rm -rf ./build && tsc -p tsconfig.json"
  },
  "peerDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/multer": "^1.4.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "coverage-badges-cli": "^1.2.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "formidable": "^3.5.1",
    "multer": "1.4.5-lts.1",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.1",
    "sagetest": "^0.1.7",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "http-status-codes": "^2.3.0"
  }
}
